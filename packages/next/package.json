{
  "name": "@next-url-rewrite/next",
  "version": "0.2.0",
  "description": "Next.js middleware for URL rewriting",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "keywords": [
    "nextjs",
    "next",
    "middleware",
    "url-rewrite",
    "routing",
    "typescript",
    "app-router",
    "pages-router"
  ],
  "author": "Dharmarajsinh Jethva",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Goku-kun/next-url-rewrite.git",
    "directory": "packages/next"
  },
  "bugs": {
    "url": "https://github.com/Goku-kun/next-url-rewrite/issues"
  },
  "homepage": "https://github.com/Goku-kun/next-url-rewrite#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "@next-url-rewrite/core": "workspace:*"
  },
  "peerDependencies": {
    "next": ">=12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "next": "^14.0.0",
    "tsdown": "^0.16.3",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  }
}
